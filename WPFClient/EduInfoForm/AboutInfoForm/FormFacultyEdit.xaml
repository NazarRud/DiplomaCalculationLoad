<Window x:Class="WPFClient.EduInfoForm.AboutInfoForm.FormFacultyEdit"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Редагування" Height="211" Width="312" ResizeMode="CanMinimize" Loaded="FacultyEdit_OnLoaded">
    <Grid>
        <Label Content="Назва факультету" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Повна назва факультету" HorizontalAlignment="Left" Margin="10,64,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="10,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="TextBoxName"/>
        <TextBox HorizontalAlignment="Left" Height="46" Margin="10,90,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="272" Name="TextBoxFullName"/>
        <Button Content="Зберегти" HorizontalAlignment="Left" Margin="10,141,0,0" VerticalAlignment="Top" Width="75" Name="SaveButton" Click="SaveButton_OnClick"/>
        <Button Content="Скасувати" HorizontalAlignment="Left" Margin="207,141,0,0" VerticalAlignment="Top" Width="75" Name="CancelButton" Click="CancelButton_OnClick"/>

    </Grid>
</Window>
